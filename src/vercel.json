    "builds": [
        {

            "src": "theme/**",
            "use": "@vercel/static"
          
        },
        {
            
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
  }