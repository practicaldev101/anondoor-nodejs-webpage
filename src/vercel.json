{
    "version": 2,
    "builds": [
        {
            
            "src": "./index.js",
            "use": "@vercel/node"
        },
        { 
            "src": "./theme/**}", 
            "use": "@vercel/static"
         }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
  }